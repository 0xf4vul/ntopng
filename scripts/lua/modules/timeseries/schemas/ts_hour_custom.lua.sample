local ts_utils = require "ts_utils_core"

local lib = {}

function lib.addCustomSeries()
   --
   -- This is an example of a hour (3600 sec) counter
   --
   local schema
   
   schema = ts_utils.newSchema("host:h_contacts", {step=3600, metrics_type=ts_utils.metrics.gauge})
   schema:addTag("ifid")
   schema:addTag("host")
   schema:addMetric("as_client")
   schema:addMetric("as_server")
end

return lib
