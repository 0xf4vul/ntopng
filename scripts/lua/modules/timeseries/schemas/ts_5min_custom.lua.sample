local ts_utils = require "ts_utils_core"

local lib = {}

function lib.addCustomSeries()
   --
   -- This is an example of a 5 minute (300 sec) counter
   --
   local schema
   
   schema = ts_utils.newSchema("host:anomalous_5m_flows", {step = 300})
   schema:addTag("ifid")
   schema:addTag("host")
   schema:addMetric("flows_as_client")
   schema:addMetric("flows_as_server")
end

return lib
